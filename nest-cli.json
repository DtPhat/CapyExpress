{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/learning/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "modules/mail/templates/**/*.hbs"
      }
    ],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/learning/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "CapyNest": {
      "type": "application",
      "root": "apps/CapyNest",
      "entryFile": "main",
      "sourceRoot": "apps/CapyNest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/CapyNest/tsconfig.app.json"
      }
    },
    "learning": {
      "type": "application",
      "root": "apps/learning",
      "entryFile": "main",
      "sourceRoot": "apps/learning/src",
      "compilerOptions": {
        "tsConfigPath": "apps/learning/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/learning"
}